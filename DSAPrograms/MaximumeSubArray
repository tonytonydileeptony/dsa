
class Solution {
  public static int MaximumeSubArray(int[] arr){
    int currentMax=0;
    int maxSoFar=arr[0];
    for(int i=0;i<arr.length;i++){
      currentMax+=arr[i];
      if(currentMax>maxSoFar){
        maxSoFar=currentMax;  

      }
      if(currentMax<0){
        currentMax=0;
      }
    }
    return maxSoFar;
  }
  public static void main(String[] args) {
  int[] nums = {-2,1,-3,4,-1,2,1,-5,4};

 System.out.print(MaximumeSubArray(nums));
  }
}
